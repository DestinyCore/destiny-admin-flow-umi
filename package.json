{
  "name": "destiny-flow",
  "version": "5.0",
  "private": true,
  "description": "Destiny Flow",
  "repository": {
    "type": "GitHub",
    "directory": "U•ェ•*U",
    "url": "https://github.com/joesdu/destiny-admin-flow-umi.git"
  },
  "scripts": {
    "start": "cross-env PORT=8848 umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "analyze": "cross-env ANALYZE=1 umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-layout": "6.4.9",
    "antd": "^4.5.4",
    "guid-typescript": "^1.0.9",
    "js-cookie": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-infinite-scroller": "^1.2.4",
    "umi": "^3.2.16"
  },
  "devDependencies": {
    "@types/history": "^4.7.7",
    "@types/js-cookie": "^2.2.6",
    "@types/prettier": "^2.0.2",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-infinite-scroller": "^1.2.1",
    "@umijs/fabric": "^2.2.2",
    "@umijs/plugin-access": "^2.3.1",
    "@umijs/plugin-antd": "^0.7.0",
    "@umijs/plugin-esbuild": "^1.0.0-beta.3",
    "@umijs/plugin-helmet": "^1.0.2",
    "@umijs/plugin-initial-state": "^2.2.1",
    "@umijs/plugin-layout": "^0.11.15",
    "@umijs/plugin-locale": "^0.9.1",
    "@umijs/plugin-model": "^2.5.3",
    "@umijs/plugin-request": "^2.4.2",
    "@umijs/preset-ant-design-pro": "^1.2.2",
    "@umijs/yorkie": "^2.0.5",
    "cross-env": "^7.0.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">=13.5.0"
  }
}
